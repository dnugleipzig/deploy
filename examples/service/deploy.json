{
  "deployment": {
    "application": "example-service"
  },
  "requirements": [
    "NETFramework451"
  ],
  "packages": {
    "NServiceBus.PowerShell": {
      "version": "4.3.0",
      "import": "lib/net40/NServiceBus.PowerShell.dll"
    }
  },
  "dns": [
    {
      "type": "a",
      "name": "example.com",
      "value": "1.2.3.4"
    }
  ],
  "permissions": {
    ".": {
      "FullControl": [
        "sid://BuiltinAdministratorsSid",
        "sid://LocalSystemSid"
      ]
    },
    "bin": {
      "ReadAndExecute": [
        "sid://NetworkServiceSid"
      ]
    },
    "logs": {
      "Modify": [
        "sid://NetworkServiceSid"
      ]
    }
  },
  "application": {
    "type": "service"
  }
}
